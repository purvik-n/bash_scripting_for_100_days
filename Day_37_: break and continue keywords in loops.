#!/bin/bash
for i in {1..10}
do
  if [ $i -eq 5 ]; then
    echo "Breaking out of the loop at $i"
    break
  fi
  echo "Number $i"
done
echo "Loop ended."


#!/bin/bash
for i in {1..5}
do
  if [ $i -eq 3 ]; then
    echo "Skipping number $i"
    continue
  fi
  echo "Number $i"
done
echo "Loop ended."
